{
  "name": "redux-ui-state",
  "version": "2.0.1",
  "description": "UI state management for Redux applications",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./dist",
    "build:lib": "tsc",
    "build:lib:watch": "tsc -w",
    "build:dist": "rollup -c",
    "build": "npm run build:lib && npm run build:dist",
    "prepublish": "npm run clean && npm run build",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest --config jest.json",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiecopeland/redux-ui-state.git"
  },
  "keywords": [
    "react",
    "redux",
    "ui",
    "state",
    "ui state",
    "higher order component",
    "higher order function"
  ],
  "author": "Jamie Copeland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamiecopeland/redux-ui-state/issues"
  },
  "homepage": "https://github.com/jamiecopeland/redux-ui-state#readme",
  "dependencies": {
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^20.0.2",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "rollup": "^0.43.0",
    "rollup-plugin-typescript2": "^0.4.5",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-jest": "^20.0.6",
    "tslint": "^4.2.0",
    "tslint-eslint-rules": "^2.2.1",
    "tslint-microsoft-contrib": "^2.0.14",
    "tslint-react": "^2.2.0",
    "typescript": "^2.3.4",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.0",
    "redux": "^3.6.0"
  }
}
