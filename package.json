{
  "name": "redux-ui-state",
  "version": "2.0.0",
  "description": "UI state management for Redux applications",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublish": "npm run build",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiecopeland/redux-ui-state.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Jamie Copeland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamiecopeland/redux-ui-state/issues"
  },
  "homepage": "https://github.com/jamiecopeland/redux-ui-state#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "<rootDir>/src/.*\\.test\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^16.0.7",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/redux": "^3.6.0",
    "enzyme": "^2.8.2",
    "jest": "^18.1.0",
    "react-addons-test-utils": "^15.0.1",
    "ts-jest": "^20.0.3",
    "tslint": "^4.2.0",
    "tslint-eslint-rules": "^2.2.1",
    "tslint-microsoft-contrib": "^2.0.14",
    "tslint-react": "^2.2.0",
    "typescript": "^2.3.4",
    "webpack": "^1.14.0"
  }
}
