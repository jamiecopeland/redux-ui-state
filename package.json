{
  "name": "redux-ui-state",
  "version": "2.0.2",
  "description": "UI state management for Redux applications",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./dist",
    "build:lib": "tsc",
    "build:lib:watch": "tsc -w",
    "build:dist": "rollup -c",
    "build": "npm run build:lib && npm run build:dist",
    "prepublish": "npm run clean && npm run build",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest --config jest.json",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiecopeland/redux-ui-state.git"
  },
  "keywords": [
    "react",
    "redux",
    "ui",
    "state",
    "ui state",
    "higher order component",
    "higher order function"
  ],
  "author": "Jamie Copeland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamiecopeland/redux-ui-state/issues"
  },
  "homepage": "https://github.com/jamiecopeland/redux-ui-state#readme",
  "peerDependencies": {
    "react": "^15.6.2 || ^16.0.0",
    "react-redux": "^5.0.7",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.9",
    "@types/react-redux": "^6.0.2",
    "jest": "^22.4.3",
    "react": "^15.6.2 || ^16.0.0",
    "react-redux": "^5.0.7",
    "reselect": "^3.0.1",
    "rollup": "^0.57.1",
    "rollup-plugin-typescript2": "^0.5.0",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-jest": "^22.4.2",
    "tslint": "^4.5.1",
    "tslint-eslint-rules": "^2.2.1",
    "tslint-microsoft-contrib": "^2.0.14",
    "tslint-react": "^2.2.0"
  }
}
